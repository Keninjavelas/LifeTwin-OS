{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-native",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@screens/*": ["screens/*"],
      "@state/*": ["state/*"],
      "@navigation/*": ["navigation/*"],
      "@models/*": ["models/*"]
    }
  },
  "include": ["src"]
}
